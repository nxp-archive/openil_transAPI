<?xml version="1.0" encoding="UTF-8"?>

<bridges xmlns="urn:ieee:std:802.1Q:yang:ieee802-dot1q-bridge"
         xmlns:sfsg="urn:ieee:std:802.1Q:yang:ieee802-dot1q-stream-filters-gates"
         xmlns:qci-augment="urn:ieee:std:802.1Q:yang:ieee802-dot1q-qci-augment"
         xmlns:psfp="urn:ieee:std:802.1Q:yang:ieee802-dot1q-psfp">
	<bridge>
        <name>enetc</name>
	<bridge-type>provider-edge-bridge</bridge-type>
        <component>
            <name>eno0</name>
	    <type>edge-relay-component</type>
            <sfsg:stream-filters>
                <sfsg:stream-filter-instance-table>
                    <sfsg:stream-filter-instance-id>1</sfsg:stream-filter-instance-id>
                    <qci-augment:stream-filter-enabled>true</qci-augment:stream-filter-enabled>
                    <sfsg:stream-handle>100</sfsg:stream-handle>
                    <sfsg:stream-gate-ref>1</sfsg:stream-gate-ref>
                </sfsg:stream-filter-instance-table>
            </sfsg:stream-filters>
            <sfsg:stream-gates>
                <sfsg:stream-gate-instance-table>
                    <sfsg:stream-gate-instance-id>2</sfsg:stream-gate-instance-id>
                    <sfsg:gate-enable>true</sfsg:gate-enable>
                    <sfsg:admin-gate-states>open</sfsg:admin-gate-states>
                    <psfp:admin-control-list-length>2</psfp:admin-control-list-length>
                    <psfp:admin-cycle-time>
			<psfp:numerator>2000</psfp:numerator>
			<psfp:denominator>1000000000</psfp:denominator>
		    </psfp:admin-cycle-time>
                    <psfp:admin-control-list>
                        <psfp:index>0</psfp:index>
                        <psfp:operation-name>psfp:set-gate-and-ipv</psfp:operation-name>
                        <psfp:parameters>
                            <psfp:gate-state-value>closed</psfp:gate-state-value>
                            <psfp:ipv-value>wildcard</psfp:ipv-value>
                            <psfp:time-interval-value>1000</psfp:time-interval-value>
                        </psfp:parameters>
                    </psfp:admin-control-list>
                    <psfp:admin-control-list>
                        <psfp:index>1</psfp:index>
                        <psfp:operation-name>psfp:set-gate-and-ipv</psfp:operation-name>
                        <psfp:parameters>
                            <psfp:gate-state-value>open</psfp:gate-state-value>
                            <psfp:ipv-value>wildcard</psfp:ipv-value>
                            <psfp:time-interval-value>1000</psfp:time-interval-value>
                        </psfp:parameters>
                    </psfp:admin-control-list>
                </sfsg:stream-gate-instance-table>
            </sfsg:stream-gates>
            <psfp:flow-meters>
                <psfp:flow-meter-instance-table>
                    <psfp:flow-meter-instance-id>2</psfp:flow-meter-instance-id>
                    <qci-augment:flow-meter-enabled>true</qci-augment:flow-meter-enabled>
                    <psfp:committed-information-rate>5000000000</psfp:committed-information-rate>
                    <psfp:committed-burst-size>1500</psfp:committed-burst-size>
                    <psfp:excess-information-rate>5000000000</psfp:excess-information-rate>
                    <psfp:excess-burst-size>1500</psfp:excess-burst-size>
                    <psfp:coupling-flag>one</psfp:coupling-flag>
                    <psfp:color-mode>color-aware</psfp:color-mode>
                    <psfp:drop-on-yellow>true</psfp:drop-on-yellow>
                </psfp:flow-meter-instance-table>
            </psfp:flow-meters>
        </component>
	</bridge>
</bridges>
